{
"extends": [
"react-app",
"react-app/jest"
],
  "rules": {
    "prefer-const": "error",
    "no-unused-vars": "warn",
    "no-console": "warn",
    "quotes": ["warn", "single"],
    "indent": ["warn", 2],
    "max-len": ["error", {"code": 120}],
    "semi": ["warn", "always"],
    "import/order": ["error", {
     "groups": ["builtin", "external", "internal", "parent", "sibling", "index", "object", "type"],
     "newlines-between": "always-and-inside-groups"
    }]
  }
}